<h1>Listing resource_views</h1>

<table>
  <tr>
    <th>Part</th>
    <th>Resource url</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @resource_views.each do |resource_view| %>
  <tr>
    <td><%= resource_view.part_id %></td>
    <td><%= resource_view.resource_url %></td>
    <td><%= resource_view.description %></td>
    <td><%= link_to 'Show', resource_view %></td>
    <td><%= link_to 'Edit', edit_resource_view_path(resource_view) %></td>
    <td><%= link_to 'Destroy', resource_view, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

